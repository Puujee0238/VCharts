<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vinctor.vcharts">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RadarActivity" />
        <activity android:name=".BarSingleActivity" />
        <activity android:name=".LineActivity" />
        <activity android:name=".BarMultiActivity" />
        <activity android:name=".RingActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".DiagragActivity" />
        <activity
            android:name=".SectorActivity"
            android:label="@string/title_activity_sector"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CompareActivity" />
        <activity android:name=".ProgressBarViewActivity" />
        <activity android:name=".DountActivity"></activity>
    </application>

</manifest>